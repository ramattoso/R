Entrada=read.csv("C:/Users/raque/Dropbox/LNCC/Mat√©rias/Estat√≠stica/Trabalho 1/dados/data1a.dat",header = FALSE)
a<-vector()
b<-vector()
i<-1
n<-5
N<-2000
for (i in 1:2000) {
wi <-sample(Entrada$V1,n)
a[i]<-var(wi)
b[i]<-mean(wi)
}
pdf("Q4",n,"X.pdf")
c<- format(round(var(b),2), nsmall=4)
d<- format(round(mean(b),2), nsmall=4)
e<- format(round(mean(Entrada$V1),2), nsmall=4)
f<- format(round(var(Entrada$V1)/n,2), nsmall=4)
hist(b,breaks=30,xlim=c(min(b),max(b)),freq=FALSE,main="PDF de X",xlab = "n=",n,"",ylab = "Frequ√™ncia relativa")
x <- seq(min(b),max(b),length=100)
y <- dnorm(x,mean=mean(Entrada$V1),sd=sqrt(var(Entrada$V1)/n))
lines(x,y, col="blue",lwd=2,add=TRUE)
legend("bottomright",c(paste("mu=",d,",var=",c),paste("mu=",e,",var=",f)), col=c("black","blue"), lwd=2, xjust = 1)
#dev.off()#
